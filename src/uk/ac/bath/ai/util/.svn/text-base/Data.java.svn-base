package uk.ac.bath.ai.util;

import java.io.Serializable;

/**
 * Class for training data.
 * 
 * image is the input packed into a single 1D array
 * label is the output.
 * 
 * If output is a class then label[i] is a number between 0 and 1 which tells us the certainty(?) of 
 *  that label.
 * 
 * @author pjl
 *
 */

public class Data implements Serializable {
    /**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	public float image[];
    public float label[];
}
